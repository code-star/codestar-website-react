{
  "name": "codestar-website-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.5",
    "@fortawesome/fontawesome-svg-core": "^1.2.13",
    "@fortawesome/free-brands-svg-icons": "^5.0.10",
    "@fortawesome/react-fontawesome": "0.1.4",
    "@material-ui/core": "^3.1.2",
    "@material-ui/icons": "^3.0.1",
    "bootstrap-css-only": "^4.1.1",
    "classnames": "^2.2.6",
    "defer-to-connect": "^1.0.1",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "history": "^4.7.2",
    "i18next": "^11.3.2",
    "i18next-browser-languagedetector": "^2.2.0",
    "i18next-xhr-backend": "^1.5.1",
    "jest-cli": "^23.6.0",
    "jest-enzyme": "^6.0.4",
    "lint-staged": "^7.0.0",
    "lodash": "^4.17.10",
    "node-fetch": "^2.2.0",
    "node-sass": "^4.10.0",
    "nuka-carousel": "^4.2.5",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-ga": "^2.5.3",
    "react-i18next": "^7.7.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^2.1.1",
    "react-scroll": "^1.7.10",
    "react-test-renderer": "^16.4.2",
    "sanitize-html": "^1.18.2",
    "simplex-noise": "^2.4.0",
    "snapsvg-cjs": "0.0.6",
    "vivus": "^0.4.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --ext .ts --ext .tsx src/ --fix",
      "prettier --write \"src/**/*.{ts,tsx}\"",
      "git add"
    ],
    "src/**/*.{js,jsx,json}": [
      "eslint --ext .js --ext .jsx src/ --fix",
      "prettier --write \"src/**/*.{js,jsx}\"",
      "git add"
    ],
    "src/**/*.{css,scss}": [
      "stylelint \"src/**/*.{css,scss}\" --syntax=scss --fix",
      "prettier --write \"src/**/*.{css,scss}\"",
      "git add"
    ]
  },
  "scripts": {
    "start": "REACT_APP_STAGE=dev react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "test:coverage": "npm run test -- --coverage --silent",
    "test:watchAll:silent": "npm run test -- --watchAll --silent",
    "test:watchAll:verbose": "npm run test -- --watchAll --verbose",
    "test:watchChanged:silent": "npm run test -- --watch --onlyChanged --silent",
    "test:watchChanged:verbose": "npm run test -- --watch --onlyChanged --verbose",
    "build": "npm run test:coverage -- --bail --ci && npm run lint:prettify && react-scripts build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -c .storybook -o .storybook-static",
    "tsc": "tsc --noEmit -p ./tsconfig.json",
    "lint": "npm-run-all lint:ts:tsx lint:js:jsx lint:css:scss",
    "lint:ts:tsx": "eslint --ext .ts --ext .tsx src/ --fix",
    "lint:js:jsx": "eslint --ext .js --ext .jsx src/ --fix",
    "lint:css:scss": "stylelint \"src/**/*.{css,scss}\" --syntax=scss --fix",
    "lint:staged": "lint-staged",
    "prettify": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,scss}\"",
    "lint:prettify": "npm-run-all lint prettify"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@storybook/addon-actions": "^5.0.5",
    "@storybook/addon-info": "^5.0.5",
    "@storybook/addon-knobs": "^5.0.5",
    "@storybook/addon-links": "^5.0.5",
    "@storybook/addon-notes": "^5.0.5",
    "@storybook/addons": "^5.0.5",
    "@storybook/react": "^5.0.5",
    "@storybook/theming": "^5.0.5",
    "@types/classnames": "^2.2.6",
    "@types/enzyme": "^3.1.13",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.9",
    "@types/lodash": "^4.14.116",
    "@types/node": "^10.12.10",
    "@types/nuka-carousel": "^4.2.2",
    "@types/prop-types": "^15.5.4",
    "@types/react": "^16.7.7",
    "@types/react-dom": "^16.0.10",
    "@types/react-i18next": "^7.8.0",
    "@types/react-router-dom": "^4.3.0",
    "@types/react-scroll": "^1.5.3",
    "@types/react-test-renderer": "^16.0.2",
    "@types/recompose": "^0.26.3",
    "@types/sanitize-html": "^1.18.0",
    "@types/storybook__react": "^4.0.1",
    "@types/vivus": "^0.4.0",
    "@typescript-eslint/eslint-plugin-tslint": "^1.5.0",
    "@typescript-eslint/parser": "^1.5.0",
    "autoprefixer": "^9.3.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-loader": "^8.0.0",
    "eslint": "5.12.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "jest-fetch-mock": "^2.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "react-docgen-typescript-loader": "^3.0.1",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "ts-jest": "^23.10.5",
    "tslint": "^5.14.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.1.6"
  }
}
