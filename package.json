{
  "name": "codestar-website-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.5",
    "@fortawesome/fontawesome-free-brands": "^5.0.10",
    "@fortawesome/fontawesome-free-regular": "^5.0.10",
    "@fortawesome/fontawesome-free-solid": "^5.0.10",
    "@fortawesome/react-fontawesome": "^0.0.18",
    "@material-ui/core": "^1.3.1",
    "@material-ui/icons": "^1.1.0",
    "bootstrap-css-only": "^4.1.1",
    "custom-react-scripts": "^0.2.2",
    "gh-pages": "^1.1.0",
    "got": "^8.3.2",
    "history": "^4.7.2",
    "husky": "^0.14.3",
    "i18next": "^11.3.2",
    "i18next-browser-languagedetector": "^2.2.0",
    "i18next-xhr-backend": "^1.5.1",
    "jest-cli": "^20.0.4",
    "lint-staged": "^7.0.0",
    "lodash": "^4.17.10",
    "nuka-carousel": "^4.2.5",
    "prettier": "^1.10.2",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-ga": "^2.5.3",
    "react-i18next": "^7.7.0",
    "react-router-dom": "^4.2.2",
    "react-scroll": "^1.7.10",
    "react-test-renderer": "^16.4.1",
    "sanitize-html": "^1.18.2",
    "serverless": "^1.28.0",
    "simplex-noise": "^2.4.0",
    "snapsvg-cjs": "0.0.6",
    "vivus": "^0.4.4"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write --trailing-comma es5",
      "git add"
    ]
  },
  "scripts": {
    "start": "REACT_APP_STAGE=dev react-scripts start",
    "build": "react-scripts test --env=jsdom --coverage --bail --ci && react-scripts build",
    "build:test": "REACT_APP_STAGE=test npm run build",
    "test": "react-scripts test --env=jsdom --coverage",
    "test:watch": "react-scripts test --env=jsdom --watch",
    "eject": "react-scripts eject",
    "precommit": "lint-staged"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  },
  "devDependencies": {
    "jest-fetch-mock": "^1.6.5"
  }
}
